@Composable
fun TasksScreen() {
    TasksViewModel
}

@Composable
fun StatisticsScreen() {
    StatisticsViewModel
}

@Composable
fun AddEditTaskScreen() {
    AddEditTaskViewModel
}

@Composable TaskDetailScreen() {
    TaskDetailViewModel
}

TodoNavGraph {
    TodoDestinations.TASKS_ROUTE         -> AppModelDrawer(TasksScreen())
    TodoDestinations.STATISTIC_ROUTE     -> AppModelDrawer(StatisticsScreen())
    TodoDestinations.ADD_EDIT_TASK_ROUTE -> AddEditTaskScreen()
    TodoDestinations.TASK_DETAIL_ROUTE   -> TaskDetailScreen()
}

TodoActivity {
    onCreate() -> TodoNavGraph
}

